    "format": "prettier --write ."
  },
  "dependencies": {
    "@elysiajs/swagger": "^1.3.1",
    "@prisma/client": "^6.11.0",
    "elysia": "latest",
    "prisma": "^6.11.0",
    "prismabox": "^1.1.24"
  },
  "devDependencies": {
    "bun-types": "latest"
  },
  "module": "src/index.ts"
}
